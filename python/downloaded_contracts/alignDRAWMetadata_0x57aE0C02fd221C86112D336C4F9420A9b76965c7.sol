contract alignDRAWMetadata is Ownable {
    uint256 private immutable MAX_SUPPLY;
    address public src = 0x233C1fB6E9a1c9eEA44eBED85faF991485848693;
    string public metadataHashDefault = "";

    /// @dev This event emits when the metadata of a range of tokens is changed. Anyone aware of ERC-4906 can update
    ///  cached attributes for tokens in the designated range.
    event BatchMetadataUpdate(uint256 fromTokenId, uint256 toTokenId);
    
    constructor(
        uint256 maxSupply
    )  Ownable(msg.sender) {
        // CHECKS inputs
        require(maxSupply > 0, "Max supply must not be zero");
        
		// EFFECTS
        MAX_SUPPLY = maxSupply;
    }
    
    /// @notice Update the Image Contract for all tokens
    /// @dev Can only be called by the contract `owner`. Emits an ERC-4906 event.
    /// @param srcContract_ The new URI base. When specified, token URIs are created by concatenating the baseURI,
    ///  token ID, and ".json".
    function updateImageContract(address srcContract_) external onlyOwner {
        // CHECKS inputs
        require(srcContract_ != address(0), "Image Contract must not be the zero address");

        // EFFECTS
        src = srcContract_;

        emit BatchMetadataUpdate(1, MAX_SUPPLY);
    }

    function updateMetadataHashDefault(string memory metadataHashDefault_) external onlyOwner {
        // CHECKS inputs
        require(bytes(metadataHashDefault_).length > 0, "Default IPFS Hash must not be empty");

        // EFFECTS
        metadataHashDefault = metadataHashDefault_;

        emit BatchMetadataUpdate(1, MAX_SUPPLY);
    }

    // VIEW FUNCTIONS

    /// @notice The URI for the given token
    /// @dev Throws if `tokenId` is not valid or has not been minted


    function readImage(uint256 id) private view returns(string memory) {
        return string.concat(
            "data:image/png;base64,",
            Base64.encode(bytes(IImageStore(src).getImage(id)))
        );
    }

   function tokenURI(uint256 tokenId) external view returns (string memory) {

        // This is assuming readImage(tokenId) returns a Base64 encoded image data URI
        string memory imageUri = readImage(tokenId); 

        // Check if the imageUri is empty or error occurred
        if (bytes(imageUri).length == 0 || keccak256(abi.encodePacked(imageUri)) == keccak256(abi.encodePacked('data:image/png;base64,'))) {
        return string.concat(
            "ipfs://",
            metadataHashDefault,
            "/",
            Strings.toString(tokenId),
            ".json"
        );
        }

        string memory attributes = string.concat(
            '[{"trait_type":"Prompt","value":"', _promptName(tokenId),'"},'
            '{"trait_type":"Prompt Type","value":"', _promptType(tokenId),'"}]'
        );

        string memory svg = string.concat(
            '<svg width="500" height="500" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">'
            '<image width="500" height="500" image-rendering="pixelated" xlink:href="', imageUri, '"/>'
            '</svg>'
        );

        // Safari compatible blur effect
        string memory html = string.concat(
            '<!DOCTYPE html>'
            '<html>'
                '<head>'
                '<style>'
                    'body, html {'
                        'margin: 0;'
                        'padding: 0;'
                        'overflow: hidden;'
                    '}'
                    'svg {'
                        'display: block;'
                        'width: 100%;'
                        'height: 100%;'
                    '}'
                    'img {'
                        'width: 100%;'
                        'height: 100%;'
                        'image-rendering: -webkit-optimize-contrast;'
                        '-ms-interpolation-mode: nearest-neighbor;'
                        'image-rendering: -moz-crisp-edges;'
                        'image-rendering: pixelated;'
                    '}'
                '</style>'
                '</head>'
                '<body style="background-color: rgb(0, 0, 0)" width="500" height="500">'
                    '<svg viewBox="0 0 500 500" xmlns="http://www.w3.org/2000/svg">'
                        '<foreignObject x="0" y="0" width="500" height="500">'
                            '<img src="', imageUri, '"/>'
                        '</foreignObject>'
                    '</svg>'
                '</body>'
            '</html>'
        );

        // Base64 encode
        string memory svgBase64Encoded = Base64.encode(bytes(svg));
        string memory htmlBase64Encoded = Base64.encode(bytes(html));

        string memory json = Base64.encode(
            bytes(
                string.concat(
                    '{"name":"', _promptName(tokenId), ' #', _outputId(tokenId), '",',
                    '"description":"[alignDRAW](https://aligndraw.fellowship.xyz/) - the first text-to-image artwork from 2015.\\n\\nBy early 2015, neural networks had mastered the art of \'image-to-text\' and could create natural language captions for images. Flipping this process, and turning text into image, was a much more complex challenge solved by 19-year old prodigy Elman Mansimov\'s alignDRAW model.\\n\\n\\nFellowship is pleased to present a special release of both print editions and **fully on-chain** NFTs of this historical artwork, containing all the original images created in 2015.\\n\\n*\\"alignDRAW images can be compared to the first fixed photographs taken by Niepce in 1826-1827.\\"* \\nDr Lev Manovich\\n\\n*\\"These images represent the birth of text prompt AI generated imagery.\\"* \\nDarius Himes, Christie\'s",',
                    '"attributes":', attributes, ',',
                    '"animation_url":"data:text/html;base64,', htmlBase64Encoded, '",',
                    '"image":"data:image/svg+xml;base64,', svgBase64Encoded, '"}'
                )
                
            )
        );

        return string.concat("data:application/json;base64,", json);
    }

    function _promptType(uint256 tokenId) private pure returns (string memory) {
        if (tokenId <= 168) {
            return "Paper Prompt";
        } else {
            return "Process Prompt";
        }
    }

    function _groupId(uint256 tokenId) private pure returns (uint256) {
        if (tokenId <= 168) {
            // Tokens 1 to 168, divided into 21 groups of 8
            return (tokenId - 1) / 8;
        } else {
            // Tokens 169 and beyond, divided into groups of 121
            return 21 + ((tokenId - 169) / 121);
        }
    }

    function _outputId(uint256 tokenId) private pure returns (string memory) {
        if (tokenId <= 168) {
            // Tokens 1 to 168, modulo 8
            uint256 promptId = (tokenId - 1) % 8 + 1;
            return Strings.toString(promptId);
        } else {
            // Tokens beyond 168, modulo 121
            uint256 offset = 168;
            uint256 promptId = (tokenId - offset - 1) % 121 + 1;
            return Strings.toString(promptId);
        }
    }

    function _promptName(uint256 tokenId) private pure returns (string memory) {
        string[42] memory promptNames = [
            "A stop sign is flying in blue skies",
            "A herd of elephants flying in the blue skies",
            "A toilet seat sits open in the grass field",
            "A person skiing on sand clad vast desert",
            "A yellow school bus parked in a parking lot",
            "A red school bus parked in a parking lot",
            "A green school bus parked in a parking lot",
            "A blue school bus parked in a parking lot",
            "The decadent chocolate desert is on the table",
            "A bowl of bananas is on the table",
            "A vintage photo of a cat",
            "A vintage photo of a dog",
            "A very large commercial plane flying in blue skies",
            "A very large commercial plane flying in rainy skies",
            "A herd of elephants walking across a dry grass field",
            "A herd of elephants walking across a green grass field",
            "A rider on a blue motorcycle in the desert",
            "A rider on a blue motorcycle in the forest",
            "A surfer, a woman, and a child walk on the beach",
            "A surfer, a woman, and a child walk on the sun",
            "A group of people walk on a beach with surf boards",
            "A group of happy elephants in the dry grass field",
            "A group of happy elephants in the green grass field",
            "A herd of elephants flying in the blue skies",
            "A toilet seat sits open in the grass field",
            "A toilet seat sits open in an empty bathroom",
            "A brown horse is grazing in a field",
            "A brown horse is grazing in a beach",
            "An airplane flying off into the distance on a clear day",
            "An airplane flying off into the distance at night",
            "A yellow school bus parked in a parking lot",
            "A yellow school bus is walking across a green grass field",
            "A yellow school bus is flying in blue skies",
            "A green school bus parked in a parking lot",
            "A blue school bus parked in a parking lot",
            "A person skiing on snow clad vast mountain",
            "A person skiing on sand clad vast desert",
            "An airplane with its landing wheels out landing",
            "A very large commercial airplane walking in the green grass field",
            "A very large commercial airplane sitting on surfboard in the water",
            "A picture of a morning sky",
            "A picture of a dark sky"
        ];
        uint256 index = _groupId(tokenId); // Ensure this is a uint256
        require(index < promptNames.length, "Index out of bounds"); // This should not give a type error
        return promptNames[index];
    }

    /// @notice Query if a contract implements an interface
    /// @dev Interface identification is specified in ERC-165. This function uses less than 30,000 gas.
    /// @param interfaceId The interface identifier, as specified in ERC-165
    /// @return `true` if the contract implements `interfaceID` and `interfaceID` is not 0xffffffff, `false` otherwise
    function supportsInterface(bytes4 interfaceId) public pure virtual returns (bool) {
        return
            interfaceId == 0x80ac58cd || // ERC-721 Non-Fungible Token Standard
            interfaceId == 0x5b5e139f || // ERC-721 Non-Fungible Token Standard - metadata extension
            interfaceId == 0x2a55205a || // ERC-2981 NFT Royalty Standard
            interfaceId == 0x49064906 || // ERC-4906 Metadata Update Extension
            interfaceId == 0x7f5828d0 || // ERC-173 Contract Ownership Standard
            interfaceId == 0x01ffc9a7; // ERC-165 Standard Interface Detection
    }
}
