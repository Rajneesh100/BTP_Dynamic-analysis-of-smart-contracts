contract EthDistribution is Ownable {
    IERC20 public token;
    IERC721A public nftContract;
    IUniswapV2Router02 public router;
    mapping(address => uint256) public claimableEth;
    mapping(address => bool) private admin;
 
    modifier onlyManager() {
        require(admin[msg.sender], "You dont have privilages");
        _;
    }
 
    constructor(address _nftContractAddress, address _tokenAddress) {
        nftContract = IERC721A(_nftContractAddress);
        token = IERC20(_tokenAddress);
        router = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        admin[msg.sender] = true;
    }
 
    function updateRevenueShare() public payable onlyManager {
        uint256 totalSupply = nftContract.totalSupply();
 
        uint256 amountPerToken = msg.value / totalSupply;
 
        for (uint256 i = 1; i <= totalSupply; ++i) {
            address tokenOwner = nftContract.ownerOf(i);
            claimableEth[tokenOwner] += amountPerToken;
        }
    }
 
    function claimEth() external {
        require(claimableEth[msg.sender] > 0, "No ETH to claim");
        require(
            token.balanceOf(msg.sender) >= tokensRequired(),
            "You do not hold enough tokens to claim"
        );
        uint256 amount = claimableEth[msg.sender];
        claimableEth[msg.sender] = 0;
 
        (bool sent, ) = msg.sender.call{value: amount}("");
        require(sent, "Failed to send Ether");
    }
 
    function viewClaimableEth(address _user) external view returns (uint256) {
        return claimableEth[_user];
    }
 
    function withdraw() external onlyManager {
        (bool sent, ) = owner().call{value: address(this).balance}("");
        require(sent, "Failed to send Ether");
    }
 
    function tokensRequired() public view returns (uint) {
        address[] memory path = new address[](2);
        path[0] = 0xA0b86991c6218b36c1d19D4a2e9Eb0cE3606eB48;
        path[1] = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
        uint[] memory amounts = router.getAmountsOut(25000000, path);
 
        path[0] = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;
        path[1] = address(token);
        uint[] memory amounts2 = router.getAmountsOut(amounts[1], path);
 
        return amounts2[1];
    }
 
    function addManager(
        address _newManager,
        bool _isManager
    ) external onlyManager {
        admin[_newManager] = _isManager;
    }
}
 
