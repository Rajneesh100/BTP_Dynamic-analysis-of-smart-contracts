        /// @solidity memory-safe-assembly
        assembly {
            // Grab the free memory pointer.
            resultA := mload(0x40)
            resultB := add(resultA, 0x40)
            // Allocate 2 words for each string (1 for the length, 1 for the byte). Total 4 words.
            mstore(0x40, add(resultB, 0x40))
            // Zeroize the length slots.
            mstore(resultA, 0)
            mstore(resultB, 0)
            // Store the lengths and bytes.
            mstore(add(resultA, 0x1f), packed)
            mstore(add(resultB, 0x1f), mload(add(add(resultA, 0x20), mload(resultA))))
            // Right pad with zeroes.
            mstore(add(add(resultA, 0x20), mload(resultA)), 0)
            mstore(add(add(resultB, 0x20), mload(resultB)), 0)
        }
    }

    /// @dev Directly returns `a` without copying.
    function directReturn(string memory a) internal pure {
        assembly {
            // Assumes that the string does not start from the scratch space.
            let retStart := sub(a, 0x20)
            let retSize := add(mload(a), 0x40)
            // Right pad with zeroes. Just in case the string is produced
            // by a method that doesn't zero right pad.
            mstore(add(retStart, retSize), 0)
            // Store the return offset.
            mstore(retStart, 0x20)
            // End the transaction, returning the string.
            return(retStart, retSize)
        }
    }
}

pragma solidity 0.8.9;

contract EthsMarketBridge is Initializable, OwnableUpgradeable, UUPSUpgradeable, ReentrancyGuardUpgradeable{

    using LibString for *;

    mapping(bytes32 =>  mapping(uint256 => bool)) private ethscriptions_signatures;
    mapping(bytes32 => bool) private ethscriptionsIds_state;

    uint256 public timeFrame;
    address public authorized_verifier;

    mapping(bytes32 => uint256) public withdraw_ethscriptionsIds_sign_time;

    event OrderInitializes(bytes32 indexed tokenId, address indexed seller, uint256 block_number);
    event OrderBought(bytes32 indexed tokenId, address indexed seller, string c_name, address indexed buyer, uint256 price, uint256 timestamp);
    event Orderwithdrawn(bytes32 indexed tokenId ,address indexed seller, uint256 timestamp, string c_name);

    event ethscriptions_protocol_TransferEthscriptionForPreviousOwner(
        address indexed previousOwner,
        address indexed recipient,
        bytes32 indexed ethscriptionId
    );
    event ethscriptions_protocol_TransferEthscription(address indexed recipient,bytes32 indexed ethscriptionId);

    event ethscriptions_protocol_CreateEthscription(
        address indexed initialOwner,
        string contentURI
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() {
        _disableInitializers();
    }

    function initialize( address _authorized_s) public initializer {
        authorized_verifier = _authorized_s;
        timeFrame = 120;

        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function getEthscripHash_deposit(
        address _sender,
        address _dumbContractAddress,
        bytes32 _depositId,
        uint256 _amount,
        uint256 _timestamp
    ) internal pure returns (bytes32) {
        bytes32 message = keccak256(abi.encodePacked(_sender, _dumbContractAddress, _depositId, _amount, _timestamp));
        bytes32 ethSignedMessage = ECDSAUpgradeable.toEthSignedMessageHash(message);
        return ethSignedMessage;
    }

    function confirmDeposit(
        address _sender,
        address _dumbContractAddress,
        bytes32 _depositId,
        uint256 _amount,
        uint256 _timestamp,
        bytes calldata _signature
    ) external nonReentrant{
        require(msg.sender == _sender, "Error: invilad sender");
        require(_dumbContractAddress != address(0x0), "Error: invilad dumbContractAddress");
        require((block.timestamp - _timestamp) < timeFrame, "Error: expired signature");
        require(ethscriptionsIds_state[_depositId] != true, "Error:, invalid state");
        
        {
            address signer_user = ECDSAUpgradeable.recover(getEthscripHash_deposit(_sender, _dumbContractAddress, _depositId, _amount, _timestamp), _signature);
            require(signer_user == authorized_verifier,"Error: invalid signature");
        }
        
        ethscriptionsIds_state[_depositId] = true;

       bytes memory out = abi.encodePacked(
            'data:application/vnd.facet.tx+json;rule=esip6,{"op":"call","data":{"to":"',
            _dumbContractAddress.toHexString(),
            '","function":"bridgeIn","args":{"to":"',
            _sender.toHexString(),
            '", "amount":"',
            _amount.toString(),
            '"}}}'
        );
        emit ethscriptions_protocol_CreateEthscription(0x00000000000000000000000000000000000FacE7, string(out));
    }

    function getEthscripHash_withdrawn(
        address _previousOwner,
        address _recipient,
        address _dumbContractAddress,
        bytes32 _withdrawalId,
        uint256 _timestamp
    ) internal pure returns (bytes32) {
        bytes32 message = keccak256(abi.encodePacked(_previousOwner, _recipient, _dumbContractAddress, _withdrawalId, _timestamp));
        bytes32 ethSignedMessage = ECDSAUpgradeable.toEthSignedMessageHash(message);
        return ethSignedMessage;
    }


    function withdraw_bridge(
        address _previousOwner,
        address _recipient, 
        address _dumbContractAddress,
        bytes32[] calldata _ethscriptionIds,
        bytes32 _withdrawalId,
        uint256 _timestamp,
        bytes calldata _signature
    ) external nonReentrant{
        require(msg.sender == _recipient, "Error: invilad recipient");
        require(_dumbContractAddress != address(0x0), "Error: invilad dumbContractAddress");
        require(_ethscriptionIds.length > 0, "Error: invilad ethscriptionIds");
        require((block.timestamp - _timestamp) < timeFrame, "Error: expired signature");
        require(ethscriptionsIds_state[_withdrawalId] != true, "Error: already processed");

        {
            address signer_user = ECDSAUpgradeable.recover(getEthscripHash_withdrawn(_previousOwner, _recipient, _dumbContractAddress, _withdrawalId, _timestamp), _signature);
            require(signer_user == authorized_verifier,"Error: invalid signature");
        }

        for (uint i = 0; i < _ethscriptionIds.length; i++) {
            bytes32 ethscriptionId = _ethscriptionIds[i];
            uint256 last_sign_time = withdraw_ethscriptionsIds_sign_time[ethscriptionId];
            require((_timestamp - last_sign_time) > timeFrame, "Error: an invalid time state");
            emit ethscriptions_protocol_TransferEthscriptionForPreviousOwner(_previousOwner, _recipient, ethscriptionId);
            withdraw_ethscriptionsIds_sign_time[ethscriptionId] = block.timestamp;
        }

        ethscriptionsIds_state[_withdrawalId] = true;

        bytes memory out = abi.encodePacked(
            'data:application/vnd.facet.tx+json;rule=esip6,{"op":"call","data":{"to":"',
            _dumbContractAddress.toHexString(),
            '","function":"markWithdrawalComplete","args":{"to":"',
            _recipient.toHexString(),
            '","withdrawalId":"',
            uint256(_withdrawalId).toHexString(),
            '"}}}'
        );
        
        emit ethscriptions_protocol_CreateEthscription(0x00000000000000000000000000000000000FacE7, string(out));
    }

    function setVerifier(address _s)external onlyOwner {
        authorized_verifier = _s;
    }


    function setTimeFrame(uint256 _t)external onlyOwner {
        timeFrame = _t;
    }

    function toBytes32(address addr) pure internal returns (bytes32) {
        return bytes32(uint256(uint160(addr)));
    }



    fallback() external {}

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
    {}

}